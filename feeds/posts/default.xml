---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>SLaks.Blog</title>
	<link rel="alternate"	type="text/html"			href="{{ site.absolueBaseUrl }}/"/>
	<link rel="self"		type="application/atom+xml"	href="{{ site.absolueBaseUrl }}{{ page.url }}"/>
	<updated>{{ site.time | date_to_rfc822 }}</updated>
	<id>tag:blogger.com,1999:blog-4137132196361303955</id>
	<author>
		<name>Schabse Laks</name>
		<uri>http://slaks.net</uri>
		<email>Dev@SLaks.Net</email>
	</author>

	{% for post in site.posts %}
	<entry>
		<id>{% if post.guid %}{{ post.guid | xml_escape }}{% else %}{{ site.absolueBaseUrl }}{{ post.path | xml_escape }}{% endif %}</id>
		<link rel="alternate" type="text/html" href="{{site.absolueBaseUrl}}{{ post.url | xml_escape }}"/>
		<title type="text">{{ post.title | xml_escape }}</title>
		<updated>{{ post.date | date_to_rfc822  }}</updated>
		<published>{% if post.updated %}{{ post.updated | date_to_rfc822  }}{% else %}{{ post.date | date_to_rfc822  }}{% endif %}</published>

		{% for categories in post.categories %}
		{% include slugify-categories %}
		<category scheme="{{ site.absolueBaseUrl }}/#" label="{{ categories | xml_escape }}" term="{{ categorySlugs | xml_escape }}" />
		{% endfor %}

		<author>
			<name>Schabse Laks</name>
			<uri>http://slaks.net</uri>
			<email>Dev@SLaks.Net</email>
		</author>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}

</feed>